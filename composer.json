{
    "name": "karbowiak/killboard",
    "description": "A killboard for EVE-Online..",
    "keywords": ["eve", "eveonline", "evekill", "killboard"],
    "homepage": "https://karbowiak.dk",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Karbowiak",
            "email": "michael@karbowiak.dk",
            "homepage": "https://karbowiak.dk"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "slim/slim": "^3.1",
        "slim/twig-view": "^2.0",
        "monolog/monolog": "^1.17",
        "nullx27/esi-php": "^1.0",
        "symfony/cache": "^3.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
	"scripts": {
        "start": "php -S 127.0.0.1:8080 -t public public/index.php",
        "test": "phpunit"
	}

}
