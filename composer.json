{
    "name": "karbowiak/killboard",
    "description": "A killboard for EVE-Online..",
    "keywords": ["eve", "eveonline", "evekill", "killboard"],
    "homepage": "https://karbowiak.dk",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Karbowiak",
            "email": "michael@karbowiak.dk",
            "homepage": "https://karbowiak.dk"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "slim/slim": "^3.8.1",
        "slim/twig-view": "^2.2.0",
        "monolog/monolog": "^1.9.1",
        "nullx27/esi-php": "^1.1",
        "symfony/cache": "^3.3.6",
        "symfony/console": "^3.3.6",
        "robmorgan/phinx": "^0.8.1",
        "chrisboulton/php-resque": "^1.2",
        "gossi/php-code-generator": "^0.4.2",
        "gossi/php-code-formatter": "dev-master",
        "tightenco/collect": "^5.4.7",
        "php-pm/php-pm": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
	"scripts": {
        "start": "php -S 127.0.0.1:8080 -t public public/index.php",
        "test": "phpunit"
	}

}
